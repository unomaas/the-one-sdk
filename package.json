{
	"name": "@unomaas/the-one-sdk",
	"version": "1.0.0",
	"description": "A lightweight SDK demo for the Lord of the Rings API (https://the-one-api.dev)",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"unpkg": "dist/index.umd.js",
	"types": "dist/types.d.ts",
	"scripts": {
		"build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
		"docs": "npx typedoc",
		"docs:md": "npx typedoc --plugin typedoc-plugin-markdown --theme markdown --out docs",
		"docs:concat": "concat-md --toc --decrease-title-levels --dir-name-as-title docs > README.md"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe.git"
	},
	"keywords": [
		"lotr",
		"lord of the rings",
		"sdk",
		"api",
		"wrapper",
		"typescript"
	],
	"author": "Ryan Maas",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		},
		"./dist/index.es.js": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		},
		"./dist/index.umd.js": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"bugs": {
		"url": "https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe/issues"
	},
	"homepage": "https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe#readme",
	"devDependencies": {
		"concat-md": "^0.5.1",
		"microbundle": "^0.15.1",
		"rimraf": "^5.0.1",
		"showdown": "^2.1.0",
		"typedoc": "^0.24.8",
		"typedoc-plugin-markdown": "^3.15.3"
	},
	"dependencies": {
		"isomorphic-unfetch": "^4.0.2"
	}
}
