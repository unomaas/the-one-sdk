{
	"name": "@unomaas/lotr-sdk",
	"version": "1.0.0",
	"description": "A lightweight SDK for the Lord of the Rings API (https://the-one-api.dev)",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"unpkg": "dist/index.umd.js",
	"types": "dist/types.d.ts",
	"scripts": {
		"build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe.git"
	},
	"keywords": ["lotr", "lord of the rings", "sdk", "api", "wrapper", "typescript"],
	"author": "Ryan Maas",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		},
		"./dist/index.es.js": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		},
		"./dist/index.umd.js": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"bugs": {
		"url": "https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe/issues"
	},
	"homepage": "https://github.com/hatchways-community/99659d2d3c9f461e87d7a6dd57ac5dbe#readme",
	"devDependencies": {
		"microbundle": "^0.15.1"
	},
	"dependencies": {
		"isomorphic-unfetch": "^4.0.2"
	}
}